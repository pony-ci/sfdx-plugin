{
  "name": "@pony-ci/sfdx-plugin",
  "description": "Pony sfdx plugin to automate application lifecycle.",
  "version": "0.0.0",
  "author": "Ondřej Kratochvíl",
  "bugs": "https://github.com/pony-ci/sfdx-plugin/issues",
  "dependencies": {},
  "devDependencies": {},
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/pony-ci/sfdx-plugin",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "BSD-3-Clause",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "topics": {
      "hello": {
        "description": "Commands to say hello."
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "pony-ci/sfdx-plugin",
  "scripts": {
    "lint": "tslint --project . --config tslint.json --format stylish",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
